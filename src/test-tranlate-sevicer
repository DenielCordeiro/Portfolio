import { HttpClient } from '@angular/common/http';
import { SessionStorageService } from 'ngx-webstorage';
import { DateAdapter } from '@angular/material';
import { DatePipe, DecimalPipe } from '@angular/common';
import { SecToDateTime } from './sec-to-date-pipe';
export declare class TranslateService {
    private http;
    private sessionsStorage;
    private adapter;
    private datePipe;
    private decimalPipe;
    private secToDateTime;
    environment: any;
    data: any;
    lang: string;
    constructor(http: HttpClient, sessionsStorage: SessionStorageService, adapter: DateAdapter<Date>, datePipe: DatePipe, decimalPipe: DecimalPipe, secToDateTime: SecToDateTime, environment: any);
    use(lang: string, idCompany?: number): Promise<{}>;
    _downloadKeylangJson(lang: string): void;
    setLanguage(lang: string): void;
    getLanguage(): any;
    translateContent(text: any, locale: string): string;
}
